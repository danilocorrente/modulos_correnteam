{
    "name": "danilocorrente/modulos_correnteam",
    "description": "Pacote Laravel/PHP para comunicação com os módulos Correnteam (OCR, etc.)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danilo Corrente",
            "email": "danilo@correnteam.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Danilocorrente\\ModulosCorrenteam\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danilocorrente\\ModulosCorrenteam\\ModulosCorrenteamServiceProvider"
            ],
            "aliases": {
                "ModulosCorrenteam": "Danilocorrente\\ModulosCorrenteam\\Facades\\ModulosCorrenteam"
            }
        }
    },
    "minimum-stability": "stable",
"prefer-stable": true
}
